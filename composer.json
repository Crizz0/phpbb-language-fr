{
	"name": "maelsoucaze/french",
	"type": "phpbb-extension",
	"description": "French language pack for phpBB 3.2",
	"homepage": "https://github.com/maelsoucaze/phpbb",
	"version": "3.0.0-dev",
	"keywords": ["locale"],
	"license": "GPL-2.0",
	"authors": [{
			"name": "Maël Soucaze",
			"email": "maelsoucaze@gmail.com",
			"homepage": "http://www.maelsoucaze.com",
			"role": "Translator"
	}],
	"require": {
		"php": ">=5.3.3",
		"composer/installers": "~1.0"
	},
	"support": {
		"issues": "https://github.com/maelsoucaze/phpbb/issues",
		"source": "https://github.com/maelsoucaze/phpbb"
	},
	"require-dev": {
		"phpbb/translation-validator": "1.3.*"
